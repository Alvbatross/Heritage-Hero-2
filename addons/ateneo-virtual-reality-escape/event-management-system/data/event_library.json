	"KeyItemDaiVoltZ": {
		"EventCategory": "KEYITEM",
		"EventPrerequisiteFlags": [],
		"EventCompletionFlags": [
			"KeyItemDaiVoltZ_Done"
		],
		"Oneshot": false,
		"EventText": {
			"Name": "Key Item",
			"Text": [
				{
					"Character": "DaiVoltz Cassette Tape",
					"Line": "Several popular Japanese animated series became an unexpected symbols of resistance when the government banned their airing in 1979.",
					"Time": 10.0
				},
				{
					"Character": "DaiVoltz Cassette Tape",
					"Line": "Officially censored due to their violence and alleged harmful effects on children, these anime portrayed heroic individuals or groups fighting against powerful, tyrannical enemies.",
					"Time": 11.5
				},
				{
					"Character": "DaiVoltz Cassette Tape",
					"Line": "The Marcos regime used and regulated media, including television, to shape public opinion.",
					"Time": 6.5
				},
				{
					"Character": "DaiVoltz Cassette Tape",
					"Line": "It tightly controlled freedom of speech, using censorship to stifle critical voices and suppress dissent.",
					"Time": 6.0
				}
			]
		},
		"EventLocations": [],
		"DialogueClickable": false,
		"AudioSourceFile": "res://src/Assets/audio/key-items/VO_nar_7KO_01_v2_SevPopJap_CasetteVer.ogg"
	},
