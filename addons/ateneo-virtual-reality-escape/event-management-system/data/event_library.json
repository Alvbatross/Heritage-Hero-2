	"RadioNutribunStart": {
		"EventCategory": "RADIO",
		"EventPrerequisiteFlags": [
			"ActionStartCookingQuest_Done"
		],
		"EventCompletionFlags": [
			"RadioNutribunStart_Done"
		],
		"Oneshot": false,
		"EventText": {
			"Name": "Radio",
			"Text": [
				{
					"Character": "Presenter",
					"Line": "Good Morning Manila! We have our special guest, Chef Bunjamin, here to teach us how to make you own homemade pandesal.",
					"Time": 9.0
				},
				{
					"Character": "Presenter",
					"Line": "So, Chef Bunjamin, how do we start?",
					"Time": 3.0
				},
				{
					"Character": "Chef Bunjamin",
					"Line": "You will need the following ingredients: wheat flour, sugar, vegetable oil, salt, and yeast.",
					"Time": 9.0
				},
				{
					"Character": "Chef Bunjamin",
					"Line": "All these ingredients can be found around your house, be sure to collect everything that you need first so that it will be easy for you to work with.",
					"Time": 11.0
				},
				{
					"Character": "Presenter",
					"Line": "Wow, I think I DO have all these ingredients at home.",
					"Time": 3.0
				}
			]
		},
